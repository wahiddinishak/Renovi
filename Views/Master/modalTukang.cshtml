@model renovi.Models.tukang


<form asp-controller="" asp-action="" id="frm">
    <input type="text" asp-for="Id" id="id" class="d-none">
    <div class="form-group row">
        <label asp-for="nama" class="col-sm-4 col-form-label col-form-label-sm"></label>
        <div class="col-sm-8">
            <input type="text" asp-for="nama" class="form-control form-control-sm">
            <span asp-validation-for="nama" class="badge badge-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="telepon" class="col-sm-4 col-form-label col-form-label-sm"></label>
        <div class="col-sm-8">
            <input asp-for="telepon" class="form-control form-control-sm" />
            <span asp-validation-for="telepon" class="badge badge-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="namaBank" class="col-sm-4 col-form-label col-form-label-sm"></label>
        <div class="col-sm-8">
            <input type="text" asp-for="namaBank" class="form-control form-control-sm">
            <span asp-validation-for="namaBank" class="badge badge-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="rekening" class="col-sm-4 col-form-label col-form-label-sm"></label>
        <div class="col-sm-8">
            <input type="text" asp-for="rekening" class="form-control form-control-sm">
            <span asp-validation-for="rekening" class="badge badge-danger"></span>
        </div>
    </div>
</form>