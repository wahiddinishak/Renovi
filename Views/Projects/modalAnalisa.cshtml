@model renovi.Models.analisaParam

<form id="frm">
    <input type="text" asp-for="id" class="d-none"/>
    <input type="text" asp-for="idActD" class="d-none" id="idActDx"/>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label col-form-label-sm" asp-for="idMaterial"></label>
        <div class="col-sm-10">
            <select class="form-control form-control-sm" id="sitem" style="width: 100%;" asp-for="idMaterial">
                <option></option>
            </select>
            <span asp-validation-for="idMaterial" class="badge badge-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label col-form-label-sm" asp-for="koefisien"></label>
        <div class="col-sm-10">
            <input type="number" class="form-control form-control-sm" min="0" asp-for="koefisien">
            <small class="text-muted">Nilai koefisien mengacu pada satuan pekerjaan.</small>
            <span asp-validation-for="koefisien" class="badge badge-danger"></span>
        </div>
    </div>
</form>